import java.util.Scanner;


public class Matek {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Kérem adjon meg egy számot: ");
        int szam1 = sc.nextInt();

        System.out.println("Kérem adjon meg egy másik számot: ");
        int szam2 = sc.nextInt();

        //Két szám közül a nagyobb
        int max = Math.max(szam1, szam2);
        System.out.println(max);

        //a két szám közül a kisebbet
        int min = Math.min(szam1, szam2);
        System.out.println(min);

        //a két szám négyzetgyökét
        Math.sqrt(szam1);
        Math.sqrt(szam2);

        //a két szám közötti eltérést
        

        //a két szám köbét
        Math.pow(szam1, 3);
        Math.pow(szam2, 3);

        //a két szám hányadosának legközelebbri egészre kerekítését
        

        //a két szám hányadosának lefele kerekítését
        Math.floor(szam1);
        Math.floor(szam2);

        //a két szám hányadosának felfelé kerekítése
        Math.ceil(szam1);
        Math.ceil(szam2);
    }
}
